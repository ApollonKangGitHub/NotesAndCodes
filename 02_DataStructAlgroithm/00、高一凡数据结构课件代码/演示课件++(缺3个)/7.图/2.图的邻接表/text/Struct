#define MAX_VERTEX_NUM 10 
enum GraphKind{DG, DN, UDG, UDN}; 
struct ElemType
{
  int adjvex; 
  InfoType *info; 
};

struct ArcNode 
{
  ElemType data; 
  ArcNode *nextarc; 
};

typedef struct 
{
  VertexType data; 
  ArcNode *firstarc; 
}VNode, AdjList[MAX_VERTEX_NUM];

struct ALGraph 
{
  AdjList vertices; 
  int vexnum, arcnum; 
  GraphKind kind; 
};
#define LNode ArcNode 
#define next nextarc 
#define forward forwardarc 
typedef ArcNode *LinkList; 

